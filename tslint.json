{
  "extends": ["tslint-react", "tslint-config-airbnb"],
  "rules": {
    "import-name": false,
    "variable-name": false,
    "jsx-boolean-value": false,
    "function-name": false,
    "object-shorthand-properties-first": false,
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "brace-style": [
      "error",
      "stroustrup",
      {
        "allowSingleLine": true
      }
    ],
    "jsx-alignment": false,
    "curly": [false, "ignore-same-line"],
    "quotemark": [false, "single", "jsx-single"],
    "trailing-comma": [true, "never"],
    "ter-indent": [false],
    "no-empty":false,
    "no-console": false,
    "no-var-keyword": false,
    "max-line-length": [true, 400],
    "arrow-parens":false

  }
}
