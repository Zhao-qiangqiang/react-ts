<!doctype html>
<html>

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <style type="text/css">
    #init-loading {
      opacity: 1;
      transition: all 500ms ease-in-out
    }

    .init-loading_wrapper {
      width: 100%;
      height: 100%;
    }

    .init-loading_content {
      margin: auto;
      position: absolute;
      top: 0;
      right: 0;
      bottom: 0;
      left: 0;
      z-index: 9999;
    }
    body .v-modal{
        z-index: 5 !important
    }
  </style>
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
    integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
  <title>存货</title>
</head>

<body>
  <!-- 新零售：#3947e6  大风车：#ff571a-->
  <div id="init-loading">
    <div class="init-loading_loading_wrapper" style="color: #ff571a">
      <div class="spinner-border init-loading_content" role="status">
      </div>
    </div>
  </div>
  <div id="root"></div>
</body>

</html>
